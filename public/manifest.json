{
  "manifest_version": 3,
  "name": "buggy_full",
  "version": "1.0",
  "description": "Description of your extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "notifications"
  ],
  "commands": {
    "add-draft": { 
      "suggested_key": {
      "default": "Ctrl+Shift+K",
      "mac": "Command+Shift+K"
    },
    "description": "Add current page to draft list"}
  },
  "background": {
    "service_worker": "background.bundle.js",
    "type": "module"
  }
}
